{
    "name": "midnight/automatic-di-module",
    "description": "Zend Framework Module for midnight/automatic-di",
    "type": "library",
    "require": {
        "php": "5.5.*",
        "midnight/automatic-di": "dev-php5.5",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-servicemanager": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rudolph Gottesheim",
            "email": "r.gottesheim@loot.at"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Midnight\\AutomaticDiModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MidnightTest\\AutomaticDiModule\\": "tests"
        }
    },
    "extra": {
        "zf": {
            "component": "Midnight\\AutomaticDiModule"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.5.9"
        }
    }
}
